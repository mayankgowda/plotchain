# PlotChain API Keys
# Copy this file to .env and fill in your API keys

# Required for Anthropic models (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Required for OpenAI models (GPT-4.1, GPT-4o)
OPENAI_API_KEY=your_openai_api_key_here

# Required for Google models (Gemini 2.5 Pro)
# Note: Google uses GOOGLE_API_KEY or GOOGLE_APPLICATION_CREDENTIALS
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Enable debug logging for Gemini API responses
# Set to "1", "true", or "yes" to enable
DEBUG_GEMINI=
